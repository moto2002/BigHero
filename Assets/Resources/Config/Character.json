bute":3,"mod":1,"characterid":3031,"addatk":45,"range":2,"atk":100,"crit":0,"name":"米诺陶斯","hp":100,"opennum":0,"addhp":45,"need":200,"opentype":0,"description":"","type":3},"1001":{"attribute":0,"mod":1,"characterid":1001,"addatk":100,"range":1,"atk":1000,"crit":10,"name":"王国的铁壁----盖伦","hp":1000,"opennum":0,"addhp":100,"need":0,"opentype":1,"description":"西部瓦伦米尔王国国防联军师团长官，因五年前帝国侵略战中的英勇表现，被王国的人民称为王国的铁壁。战争过去了五年，王国已经开始逐渐恢复生气，但是北方帝国对王国的威胁始终没有消除。为了自己的祖国，为了祖国的人民，盖伦决定踏上寻找龙珠的旅程。","type":1},"1002":{"attribute":0,"mod":1,"characterid":1002,"addatk":110,"range":1,"atk":1200,"crit":10,"name":"远古部族的巫女----微斯琪","hp":1000,"opennum":"","addhp":100,"need":20,"opentype":2,"description":"微斯琪是古老的海风一族的巫女，她拥有海风一族传承的强大魔力。微斯琪有一位青梅竹马的恋人，但是她的恋人在一次出海的途中遭遇了台风，从此杳无音讯。微斯琪坚信她的恋人还活着，为了再度和恋人相见，她决定去寻找传说中的龙珠。","type":1},"1003":{"attribute":0,"mod":1,"characterid":1003,"addatk":120,"range":1,"atk":1200,"crit":20,"name":"菜鸟冒险家----伊泽","hp":1000,"opennum":30,"addhp":110,"need":50,"opentype":3,"description":"伊泽出生在蓬莱山脚一座平穷的村庄，他是村里的孩子王，所有的孩子都以伊泽马首是瞻。但是大人们却不怎么待见他，他们认为这个爱到处冒险的孩子是个疯子，现在连伊泽的父母也无法理解自己孩子的想法了。伊泽为了证明自己，决定踏上旅途，他决定要做出一番大事，证明自己。","type":1},"1004":{"attribute":0,"mod":1,"characterid":1004,"addatk":130,"range":1,"atk":1200,"crit":20,"name":"圣光的守护----乌瑟尔","hp":1000,"opennum":80,"addhp":120,"need":100,"opentype":3,"description":"作为帝国最神圣的组织--圣光骑士团的团长，乌瑟尔对自己的地位十分自负，他不允许任何人亵渎他的信仰。然而五年前帝国的侵略让他明白，自己的信仰心还不足以壮大自己的祖国，他需要更强大，更虔诚的力量。为此，他必须收集七颗龙珠来实现他的愿望。","type":1},"1005":{"attribute":0,"mod":1,"characterid":1005,"addatk":140,"range":1,"atk":1200,"crit":30,"name":"众星的眷顾----索纳塔","hp":1000,"opennum":80,"addhp":120,"need":200,"opentype":4,"description":"作为联邦的首席占星师，索纳塔放弃了她的名誉和地位，遵循繁星的指引，来到了蓬莱山，这里有解除她多年疑问的秘密，也有改变世界走向的力量。","type":1},"1006":{"attribute":0,"mod":1,"characterid":1006,"addatk":150,"range":1,"atk":1200,"crit":40,"name":"来自岛国的忍者----卡嘉莉","hp":1000,"opennum":0,"addhp":120,"need":200,"opentype":0,"description":"卡嘉莉作为东岛最大的忍者集团“浓雾”的首领，曾帮助东岛的领主統一整个岛屿。但是在现在这种没有纷争的太平盛世，忍者已经没有了存在的必要。遵循组训的卡嘉莉决定离开岛屿挑战自我，她选择的目的地正是冒险者云集的蓬莱山。